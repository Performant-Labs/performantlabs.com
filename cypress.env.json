{
    "env": {
        "blockHosts": [
            "*.google-analytics.com",
            "*.googletagmanager.com",
            "*.stats.g.doubleclick.net",
            "*.rum-collector-2.pingdom.net"
        ],
        "downloadsFolder": "cypress/downloads"
    },
    "config": {
        "drushPrefix": "fin "
    },
    "browserAuthentication": {
        "username": "aUser",
        "password": "aPassword"
    },
    "roles": {
        "administrator": "administrator"
    },
    "url": {
        "login": "user/login"
    },
    "account": {
      "admin":
        {
          "accountType": "admin",
          "userName" : "qa_administrator",
          "userPassword" : "qa_administrator"
        },
      "authenticated":
        {
          "accountType": "authenticated",
          "userName" : "qa_authenticated",
          "userPassword" : "qa_authenticated"
        }
    },
    "translationsFolder": "docroot/sites/default/files",
    "translations": {
      "ja": {
        "file": "ja-xlations.json"
      },
      "zh": {
        "file": "zh-hans-xlations.json"
      }
    }
}
